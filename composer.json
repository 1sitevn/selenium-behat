{
  "name": "onesite/selenium-behat",
  "description": "Onesite Selenium Behat",
  "license": "MIT",
  "authors": [
    {
      "name": "1Site.vn",
      "email": "hotro@1site.vn",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "elkan/behatformatter": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-goutte-driver": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "OneSite\\SeleniumBehat\\": "features/bootstrap"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OneSite\\SeleniumBehat\\": [
        "features/bootstrap",
        "tests"
      ]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x.x-dev"
    }
  }
}
